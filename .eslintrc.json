{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react", "react-hooks", "prettier"],
	"rules": {
		"react/prop-types": [0, {"ignore": ["children"]}],
		"eqeqeq": "warn",
		"comma-dangle": 0,
		"no-unused-vars": ["warn", {"varsIgnorePattern": "^h$"}]
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
